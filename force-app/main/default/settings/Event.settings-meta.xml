<?xml version="1.0" encoding="UTF-8"?>
<EventSettings xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bypassMeteringBlock>false</bypassMeteringBlock>
    <enableDeleteMonitoringData>true</enableDeleteMonitoringData>
    <enableDynamicStreamingChannel>false</enableDynamicStreamingChannel>
    <enableEventLogGeneration>true</enableEventLogGeneration>
    <enableEventLogObjectFeature xsi:nil="true"/>
    <enableEventLogWaveIntegration>true</enableEventLogWaveIntegration>
    <enableLightningLoggerEvents>true</enableLightningLoggerEvents>
    <enableLoginForensics>true</enableLoginForensics>
    <enableStreamingApi>true</enableStreamingApi>
    <enableTransactionSecurityPolicies>true</enableTransactionSecurityPolicies>
    <eventLogRetentionDuration>365</eventLogRetentionDuration>
</EventSettings>
